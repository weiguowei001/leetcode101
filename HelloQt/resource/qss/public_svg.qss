/*UI规范 QSS的实现*/

.QListWidget::item:hover,
.QTreeView::item:hover,
.QTreeWidget::item:hover{  
    background-color:#00cdc0;
}

.QListWidget::item:selected,
.QTreeView::item:selected,
.QTreeWidget::item:selected{  
    background-color:#00aca1;
}

QRadioButton
{
    padding-left: 4px;
}
QRadioButton::indicator {
    width: 14px;
    height: 14px;
}
QRadioButton::indicator:unchecked {
    image: url(:/common/image/common/radioButton/ic_radio_unchecked_normal.png);
}
QRadioButton::indicator:unchecked:hover {
    image: url(:/common/image/common/radioButton/ic_radio_hover.png);
}
QRadioButton::indicator:unchecked:pressed {
    image: url(:/common/image/common/radioButton/ic_radiobutton_unchecked_pressed.png);
}
QRadioButton::indicator:checked {
    image: url(:/common/image/common/radioButton/ic_radio_check_normal.png);
}
QRadioButton::indicator:checked:hover {
    image: url(:/common/image/common/radioButton/ic_radio_check_hover.png);
}
QRadioButton::indicator:checked:pressed {
    image: url(:/common/image/common/radioButton/ic_radiobutton_checked_pressed.png);
}
QRadioButton::indicator:unchecked:disabled {
  image: url(:/common/image/common/radioButton/ic_radiobutton_unchecked_disabled.png);
}
QRadioButton::indicator:checked:disabled {
  image: url(:/common/image/common/radioButton/ic_radiobutton_checked_disabled.png);
}

QCheckBox{
    spacing: 5px;
}

QGroupBox::title {
    subcontrol-origin:  margin;
    subcontrol-position: top left;    
    padding-left:0 2px 0 2px;
	
}

QGroupBox::indicator,
QCheckBox::indicator {
    width: 14px;
    height: 14px;
}

QGroupBox::indicator:unchecked,
QCheckBox::indicator:unchecked {
  image: url(:/common/image/common/checkBox/ic_checkbox_unchecked_normal.png);
}

QGroupBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:hover {
  image: url(:/common/image/common/checkBox/ic_checkbox_unchecked_hover.png);
}

QGroupBox::indicator:unchecked:pressed,
QCheckBox::indicator:unchecked:pressed {
  image: url(:/common/image/common/checkBox/ic_checkbox_unchecked_pressed.png);
}

QGroupBox::indicator:checked,
QCheckBox::indicator:checked {
  image: url(:/common/image/common/checkBox/ic_checkbox_checked_normal.png);
}

QGroupBox::indicator:checked:hover,
QCheckBox::indicator:checked:hover {
  image: url(:/common/image/common/checkBox/ic_checkbox_checked_hover.png);
}

QGroupBox::indicator:checked:pressed,
QCheckBox::indicator:checked:pressed {
  image: url(:/common/image/common/checkBox/ic_checkbox_checked_pressed.png);
}

QLineEdit, 
QLineEdit:disabled {
    min-height:24px;
    max-height:24px;
    border-radius:2px;
    border:1px solid #cccccc;
}

QLineEdit:hover, 
QLineEdit:focus {
    min-height:24px;
    max-height:24px;
    border-radius:2px;
    border:1px solid #009188;
}

.QTextEdit, 
.QTextEdit:disabled {
    border-radius:2px;
    border:1px solid #cccccc;
}

.QTextEdit:hover, 
.QTextEdit:focus {
    border-radius:2px;
    border:1px solid #009188;
}


