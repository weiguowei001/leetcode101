/*全局滚动条样式*/

QScrollBar:vertical
{
    background: transparent;
    width: 6px;
    border: 0px;
    border-radius: 3px;
}
QScrollBar::handle:vertical
{
    background-color: #c3c0b9;
    border-radius: 3px;
    min-height:60px;
}

QScrollBar::handle:vertical:hover
{
    background-color: #686868;
    min-height:60px;
}

QScrollBar::handle:vertical:pressed
{
    background-color: #686868;
    min-height:60px;
}

QScrollBar:horizontal
{
    height: 6px;
    background: transparent;
    border: 0px;
    border-radius: 3px;
}
QScrollBar::handle:horizontal
{
    background-color: #c3c0b9;
    border-radius: 3px;
    min-width:60px;
}

QScrollBar::handle:horizontal:hover
{
    background: #686868;
    min-width:60px;
}

QScrollBar::handle:horizontal:pressed
{
    background: #686868;
    min-width:60px;
}

QScrollBar::add-line, QScrollBar::sub-line
{
    background: transparent;
    height: 0px;
    width: 0px;
}

QScrollBar::add-page, QScrollBar::sub-page
{
    background: transparent;
    height: 0px;
    width: 0px;
}

.QScrollBar[barStyle = "commonHorizontalStyle"]:horizontal{
    background:#f0f0f0;
    border:1pxsolid#cccccc;
    border-right:none;
    padding-top:1px;
    padding-bottom:1px;
    padding-left:10px;
    padding-right:10px;
    border-radius:0px;
    height:15px;
}

.QScrollBar[barStyle = "commonHorizontalStyle"]::handle:horizontal{
    background:#bdbdbd;
    border-radius:0px;
    height:13px;
    min-width:80px;
}

.QScrollBar[barStyle = "commonHorizontalStyle"]::handle:horizontal:hover{
    background:#a1a1a1;
    border-radius:0px;
    height:13px;
    min-width:80px;
}

.QScrollBar[barStyle = "commonHorizontalStyle"]::sub-line:horizontal
{
    border:none;
    width:10px;
    height:10px;
    subcontrol-position:left;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_left_normal.png);
}

.QScrollBar[barStyle = "commonHorizontalStyle"]::sub-line:horizontal:hover
{
    border:none;
    width:10px;
    height:10px;
    subcontrol-position:left;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_left_hover.png);
}
.QScrollBar[barStyle = "commonHorizontalStyle"]::add-line:horizontal
{
    border:none;
    width:10px;
    height:10px;
