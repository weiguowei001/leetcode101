/*combobox样式*/
.QComboBox[comboxStyle = "combox_1"]{
    /*min-height:1.8em;*/
    min-height:24px;
    max-height:24px;
    border-radius:2px;
    border:1px solid #cccccc;
    font-size:9pt;
}

.QComboBox[comboxStyle = "combox_1"]:hover,
.QComboBox[comboxStyle = "combox_1"]:focus{
    min-height:24px;
    max-height:24px;
    border:1px solid #009188;
    border-radius:2px;
}

.QComboBox[comboxStyle = "combox_1"]:open {
    min-height:24px;
    max-height:24px;
    border-radius:2px;
    border:1px solid #009188;
}

.QComboBox[comboxStyle = "combox_1"]::down-arrow {
    image: url(:/common/image/common/ic_small_triangle_black_normal.png);
}
.QComboBox[comboxStyle = "combox_1"]::drop-down {
  border:0px;
  background:transparent;
  subcontrol-origin: padding;
  subcontrol-position: top right;
}

.QComboBox[comboxStyle = "combox_1"] QAbstractItemView{
  outline:0px;/*去掉虚线*/
}

.QComboBox[comboxStyle = "combox_1"] QAbstractItemView::item{
    padding:1 0 1 0;
    background:white;
    color:black;
    font-size:9pt;
}

.QComboBox[comboxStyle = "combox_1"] QAbstractItemView::item:selected{
    background:#009188;
    color:white;
}
/*combobox样式 不固定背景颜色*/
.QComboBox[comboxStyle = "combox_2"]{
    /*min-height:1.8em;*/
    border:1px solid #cccccc;
