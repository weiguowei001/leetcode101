/*全局滚动条样式*/

QScrollBar:vertical
{
    background: transparent;
    width: 9px;
    border: 0px;
    border-radius: 4px;
}
QScrollBar::handle:vertical
{
    background-color: #c3c0b9;
    border-radius: 4px;
    min-height:60px;
}

QScrollBar::handle:vertical:hover
{
    background-color: #686868;
    min-height:60px;
}

QScrollBar::handle:vertical:pressed
{
    background-color: #686868;
    min-height:60px;
}

QScrollBar:horizontal
{
    height: 9px;
    background: transparent;
    border: 0px;
    border-radius: 4px;
}
QScrollBar::handle:horizontal
{
    background-color: #c3c0b9;
    border-radius: 4px;
    min-width:60px;
}

QScrollBar::handle:horizontal:hover
{
    background: #686868;
    min-width:60px;
}

QScrollBar::handle:horizontal:pressed
{
    background: #686868;
    min-width:60px;
}

QScrollBar::add-line, QScrollBar::sub-line
{
    background: transparent;
    height: 0px;
    width: 0px;
}

QScrollBar::add-page, QScrollBar::sub-page
{
    background: transparent;
	height: 0px;
    width: 0px;
}

QTreeView[backStyle = "lightGray"]{
        background: #eef1fc;
        border: 0px;
}

QTreeView[backStyle = "lightGray"]::item{
        height: 40px;
}

QTreeView[titleStyle = "grayTransparent"] QHeaderView::section:horizontal{
    background: transparent;
	border-style: solid; 
	border-width: 0px 1px 1px 0px;
	border-color: #D8D8D8;
}

QTreeWidget[treeWidgetStyle = "TreeWidgetTitle"]{
    border:1px solid black;
}

QTreeWidget[treeWidgetStyle = "TreeWidgetTitle"] QHeaderView{
    border:1px solid black;
    border-left:hide;
    border-right:hide;
    border-top:hide;
}

.QTabWidget[tabBarStyle = "barStyle"] QTabBar::tab:!first{
    min-height: 20px;
	min-width:65px;
	border-style:solid;
	border-width: 1px 1px 1px 0px;
	padding:0 10px 0 10px
}

.QTabWidget[tabBarStyle = "barStyle"] QTabBar::tab:first{
    min-height: 20px;
	min-width:65px;
	border-style:solid;
	border-width: 1px 1px 1px 1px;
	padding:0 10px 0 10px
}

.QTabWidget[tabBarStyle = "barStyle"] QTabBar::tab:enabled:!selected{
    background-color: white;
	border-color:black;
	color:black;
}

.QTabWidget[tabBarStyle = "barStyle"] QTabBar::tab:enabled:selected{
    background-color: #1483e3;
	border-color:#1483e3;
	color:white;
}

.QTabWidget[tabBarStyle = "barStyle"] QTabBar::tab:!enabled{
    background:#F0F0F0;
	border-color:#D8D8D8;
}

