QToolButton[styleToolBtn = "style"]{
    background-color:#f4f4f4;
    min-height:16px;
    max-height:16px;
    text-align:top;
    border: none;
    border-top:3px solid #f4f4f4;
    border-radius:2px;
    padding-top:19px;
    padding-left:3px;
    padding-right:3px;   
    background-repeat: no-repeat;
    background-position: top;
    background-origin: padding;
}

QToolButton[styleToolBtn = "style"]:hover{
	background-color:#dbdbdb;
	border-top:3px solid #dbdbdb;
}

QToolButton[styleToolBtn = "style"]:pressed{
	background-color:#b7b7b7;
	border-top:3px solid #b7b7b7;
}

QToolButton[styleToolBtn = "style"]:checked{
	background-color:#cccccc;
	border-top:3px solid #cccccc;
}

QToolButton[styleToolBtn = "style"]:disabled{
	border-top:3px solid #f4f4f4;
}

