    background:#e68220;
    margin-left:6px;
	color:white;
}

.QPushButton[btnStyle = "btn_cancelEnabled"]:enabled{
	min-height:25px;
	max-height:25px;
    background:white;
    border:1px solid #1483e3;
	color:#1483e3;
	padding:0 10px 0 10px;
}
.QPushButton[btnStyle = "btn_cancelEnabled"]:hover:enabled,.QPushButton[btnStyle = "btn_cancelEnabled"]:pressed:enabled{
    background:#1483e3;
	color:white;
}

.QPushButton[btnStyle = "btn_boldCancel"]{
	min-height:25px;
	max-height:25px;
    background:white;
    border:2px solid #1483e3;
	color:#1483e3;
	font-weight:bold;
	padding:0 10px 0 10px;
}
.QPushButton[btnStyle = "btn_boldCancel"]:hover,.QPushButton[btnStyle = "btn_boldCancel"]:pressed{
    background:#1483e3;
	color:white;
}

.QPushButton[btnStyle = "btn_smallok"]{
	min-height:18px;
	max-height:18px;
    background:#1483e3;
    border:1px solid #1483e3;
	color:white;
	padding:0 10px 0 10px;
}
.QPushButton[btnStyle = "btn_smallok"]:hover,.QPushButton[btnStyle = "btn_smallok"]:pressed{
    background:#0f6dbe;
	border:1px solid #0f6dbe;
}


.QPushButton[btnStyle = "btn_ok"]:disabled{
	min-height:25px;
	max-height:25px;
    background:#E1E1E1;
    border:1px solid #E1E1E1;
	color:#585858;
	padding:0 20px 0 20px;
}

