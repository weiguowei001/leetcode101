    background:transparent;
    border:1px solid transparent;
	color:#1483e3;
	padding:0 0 0 0;
	margin: 0 10 0 10;
}
.QPushButton[btnStyle = "btn_blue"]:hover,.QPushButton[btnStyle = "btn_blue"]:pressed{
	border-bottom:1px solid #1483e3;
}

/*无边框 红色字 透明底 */
.QPushButton[btnStyle = "btn_red"]{
    background:transparent;
    border:1px solid transparent;
	color:#dd4d4d;
	padding:0 0 0 0;
	margin: 0 10 0 10;
}
.QPushButton[btnStyle = "btn_red"]:hover,.QPushButton[btnStyle = "btn_red"]:pressed{
	border-bottom:1px solid #dd4d4d;
}

