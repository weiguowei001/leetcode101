.QPushButton[btnStyle = "btn_ok_icon"]{
    background:#1483e3;
    border:1px solid #1483e3;
	color:white;
	padding:0 66px 0 66px;
	text-align:right;
}
.QPushButton[btnStyle = "btn_ok_icon"]:hover,.QPushButton[btnStyle = "btn_ok_icon"]:pressed{
    background:#0f6dbe;
	border:1px solid #0f6dbe;
}

QPushButton[imageIcon = "import"]{
	image:url(:/datapage/image/datapage/ic_daoru.png);
	image-position:left;
