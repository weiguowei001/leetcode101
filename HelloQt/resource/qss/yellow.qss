	padding:0 1.33em 0 1.33em;
}
.QPushButton[btnStyle = "btn_3"]:hover,.QPushButton[btnStyle = "btn_3"]:pressed{
    background:#D3E5FF;
	border:1px solid #2f68bc;
}

.QPushButton[btnStyle = "btn_4"]{
    min-width:7.66em;
