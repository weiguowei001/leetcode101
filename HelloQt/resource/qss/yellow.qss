/*UI规范 QSS的实现*/


/*通用UI样式Begin*/
/*UI:QLineEdit QComboBox QScrollBar DoubleSpinBox(QDoubleSpinBox QDateTime...) QTableView */

  QCheckBox[commonstyle = "checkBox"]{
      spacing: 5px;
  }

  QCheckBox[commonstyle = "checkBox"]::indicator {
      width: 12px;
      height: 12px;
  }

  QCheckBox[commonstyle = "checkBox"]::indicator:unchecked {
	image: url(:/common/image/common/checkBox/ic_checkbox_unchecked_normal.png);
  }

  QCheckBox[commonstyle = "checkBox"]::indicator:unchecked:hover {
	image: url(:/common/image/common/checkBox/ic_checkbox_unchecked_hover.png);
  }

  QCheckBox[commonstyle = "checkBox"]::indicator:unchecked:pressed {
	image: url(:/common/image/common/checkBox/ic_checkbox_unchecked_pressed.png);
  }

  QCheckBox[commonstyle = "checkBox"]::indicator:checked {
	image: url(:/common/image/common/checkBox/ic_checkbox_checked_normal.png);
  }

  QCheckBox[commonstyle = "checkBox"]::indicator:checked:hover {
	image: url(:/common/image/common/checkBox/ic_checkbox_checked_hover.png);
  }

  QCheckBox[commonstyle = "checkBox"]::indicator:checked:pressed {
	image: url(:/common/image/common/checkBox/ic_checkbox_checked_pressed.png);
  }

QLineEdit[commonstyle = "lineEdit"] {
    min-height:24px;
    max-height:24px;
    border-radius:2px;
    border:1px solid #868686;
}

QLineEdit[commonstyle = "lineEdit"]:hover,
QLineEdit[commonstyle = "lineEdit"]:focus  {
    border:1px solid #e68220;
}


QLineEdit[commonstyle = "lineEdit"]:disabled  {
    border:1px solid #c4c4c4;
    color: #c4c4c4;
}

QTextEdit[commonstyle = "textEdit"] {
    border-radius:2px;
    border:1px solid #868686;
}

QTextEdit[commonstyle = "textEdit"]:hover,
QTextEdit[commonstyle = "textEdit"]:focus  {
    border:1px solid #e68220;
}

QTextEdit[commonstyle = "textEdit"]:disabled  {
    border:1px solid #c4c4c4;
    color: #c4c4c4;
}

QPlainTextEdit[commonstyle = "plainTextEdit"] {
    border-radius:2px;
    border:1px solid #868686;
}

QPlainTextEdit[commonstyle = "plainTextEdit"]:hover,
QPlainTextEdit[commonstyle = "plainTextEdit"]:focus  {
    border:1px solid #e68220;
}

QPlainTextEdit[commonstyle = "plainTextEdit"]:disabled  {
    border:1px solid #c4c4c4;
    color: #c4c4c4;
}

QComboBox[commonstyle = "comboBox"]{
    min-height:24px;
    max-height:24px;
    border-radius:2px;
    border:1px solid #868686;
    font-size:9pt;
    padding-left:2px;
    padding-right:4px;
}

QComboBox[commonstyle = "comboBox"]:hover,
QComboBox[commonstyle = "comboBox"]:focus{
    border:1px solid #e68220;
    border-radius:2px;
}

QComboBox[commonstyle = "comboBox"]:open {
    border-radius:2px;
    border:1px solid #868686;
}

QComboBox[commonstyle = "comboBox"]::down-arrow {
    image: url(:/common/image/common/ic_small_triangle_black_normal.png);
}

QComboBox[commonstyle = "comboBox"]::drop-down {
  border:0px;
  background:transparent;
  subcontrol-origin: padding;
  subcontrol-position: top right;
}

QComboBox[commonstyle = "comboBox"] QAbstractItemView{
  outline:0px;/*去掉虚线*/
}

QComboBox[commonstyle = "comboBox"] QAbstractItemView::item{
    padding:1 0 1 0;
    background:white;
    color:black;
    font-size:9pt;
}

QComboBox[commonstyle = "comboBox"] QAbstractItemView::item:selected{
    background:#e68220;
    color:white;
}

.QScrollBar[commonstyle = "horizontalScrollBar"]:horizontal{
    background:#f0f0f0;
    border:1pxsolid#cccccc;
    border-right:none;
    padding-top:1px;
    padding-bottom:1px;
    padding-left:10px;
    padding-right:10px;
    border-radius:0px;
    height:15px;
}

.QScrollBar[commonstyle = "horizontalScrollBar"]::handle:horizontal{
    background:#bdbdbd;
    border-radius:0px;
    height:13px;
    min-width:80px;
}

.QScrollBar[commonstyle = "horizontalScrollBar"]::handle:horizontal:hover{
    background:#a1a1a1;
    border-radius:0px;
    height:13px;
    min-width:80px;
}

.QScrollBar[commonstyle = "horizontalScrollBar"]::sub-line:horizontal
{
    border:none;
    width:10px;
    height:10px;
    subcontrol-position:left;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_left_normal.png);
}

.QScrollBar[commonstyle = "horizontalScrollBar"]::sub-line:horizontal:hover
{
    border:none;
    width:10px;
    height:10px;
    subcontrol-position:left;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_left_hover.png);
}
.QScrollBar[commonstyle = "horizontalScrollBar"]::add-line:horizontal
{
    border:none;
    width:10px;
    height:10px;
    subcontrol-position:right;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_right_normal.png);
}
.QScrollBar[commonstyle = "horizontalScrollBar"]::add-line:horizontal:hover
{
    border:none;
    width:10px;
    height:10px;
    subcontrol-position:right;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_right_hover.png);
}

.QScrollBar[commonstyle = "verticalScrollBar"]:vertical{
    background:#f0f0f0;
    border:1pxsolid#cccccc;
    border-bottom:none;
    padding-left:1px;
    padding-right:1px;
    padding-top:10px;
    padding-bottom:10px;
    width:15px;
    border-radius:0px;
}

.QScrollBar[commonstyle = "verticalScrollBar"]::handle:vertical{
    background:#bdbdbd;
    border-radius:0px;
    width:13px;
    min-height:80px;
}

.QScrollBar[commonstyle = "verticalScrollBar"]::handle:vertical:hover{
    background:#a1a1a1;
    border-radius:0px;
    width:13px;
    min-height:80px;
}

.QScrollBar[commonstyle = "verticalScrollBar"]::sub-line:vertical
{
    border:none;
    width:15px;
    height:10px;
    subcontrol-position:top;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_up_normal.png);
}

.QScrollBar[commonstyle = "verticalScrollBar"]::sub-line:vertical:hover
{
    border:none;
    width:15px;
    height:10px;
    subcontrol-position:top;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_up_hover.png);
}
.QScrollBar[commonstyle = "verticalScrollBar"]::add-line:vertical
{
    border:none;
    width:15px;
    height:10px;
    subcontrol-position:bottom;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_down_normal.png);
}
.QScrollBar[commonstyle = "verticalScrollBar"]::add-line:vertical:hover
{
    border:none;
    width:15px;
    height:10px;
    subcontrol-position:bottom;
    subcontrol-origin:margin;
    background-repeat:no-repeat;
    background-position:center;
    background-image:url(:/datapage/image/datapage/ic_scroll_bar_down_hover.png);
}

QWidget[commonstyle = "doubleSpinBox"] {
    min-height:24px;
    max-height:24px;
    border:1px solid #868686;
    border-radius:2px;
}

QWidget[commonstyle = "doubleSpinBox"]:hover,
QWidget[commonstyle = "doubleSpinBox"]:focus {
    border:1px solid #e68220;
}

QWidget[commonstyle = "doubleSpinBox"]:disabled {
    border:1px solid #c4c4c4;
    color: #c4c4c4;
}

QWidget[commonstyle = "doubleSpinBox"]::up-button {
	background-color:transparent;
}
  
QWidget[commonstyle = "doubleSpinBox"]::up-arrow {
	image:url(:/common/image/common/ic_up_time_normal.png);
}

QWidget[commonstyle = "doubleSpinBox"]::up-arrow:hover,
QWidget[commonstyle = "doubleSpinBox"]::up-arrow:pressed  {
	image:url(:/common/image/common/ic_up_time_click.png);

}

QWidget[commonstyle = "doubleSpinBox"]::down-button {
	background-color:transparent;
}
  
QWidget[commonstyle = "doubleSpinBox"]::down-arrow {
	image:url(:/common/image/common/ic_down_time_normal.png);
}

QWidget[commonstyle = "doubleSpinBox"]::down-arrow:hover,
QWidget[commonstyle = "doubleSpinBox"]::down-arrow:pressed {
	image:url(:/common/image/common/ic_down_time_click.png);

}

QTableView[commonstyle = "tableView"]
{
    color: #434343;
    gridline-color: #BCBCBC;
    background-color: white;
    selection-color: #434343;
    selection-background-color: #E1E7EB;
    border: 1px groove #E4E4E4;
    border-radius: 0px;
}

QTableView[commonstyle = "tableView"]::item:!alternate:!selected
{
    background-color: white;
}

QTableView[commonstyle = "tableView"]::item:alternate:!selected
{
    background-color: #F5F5F5;
}

QAbstractButton[commonstyle = "keyBtn"],
QPushButton[commonstyle = "keyBtn"]{
    min-height:26px;
    max-height:26px;
	border: none;
	background-color: rgb(230, 130, 32);
	padding-left:25px;
	padding-right:25px;
    border-radius:2px;
    color:white;
    font-family:"Microsoft YaHei";
    font-size:12px;
}

QAbstractButton[commonstyle = "keyBtn"]:hover,
QPushButton[commonstyle = "keyBtn"]:hover{
	background-color: rgb(255, 164, 73);
}

QAbstractButton[commonstyle = "keyBtn"]:pressed,
QPushButton[commonstyle = "keyBtn"]:pressed{
	background-color: rgb(190, 100, 12);
}

QAbstractButton[commonstyle = "keyBtn"]:checked,
QPushButton[commonstyle = "keyBtn"]:checked{

}

QAbstractButton[commonstyle = "keyBtn"]:disabled,
QPushButton[commonstyle = "keyBtn"]:disabled{
	background-color: rgb(196, 196, 196);
}

QAbstractButton[commonstyle = "textBtn"],
QPushButton[commonstyle = "textBtn"]{
    min-height:24px;
    max-height:24px;
	background-color: rgb(255, 255, 255);
	padding-left:25px;
	padding-right:25px;
    border-radius:2px;
	border:1px solid #868686;
	color:#868686;
    font-family:"Microsoft YaHei";
    font-size:12px;
}

QAbstractButton[commonstyle = "textBtn"]:hover,
QPushButton[commonstyle = "textBtn"]:hover{
	border:1px solid #ffa44a;
	color:#ffa44a;
}

QAbstractButton[commonstyle = "textBtn"]:pressed,
QPushButton[commonstyle = "textBtn"]:pressed{
	border:1px solid #be640c;
	color:#be640c;
}

QAbstractButton[commonstyle = "textBtn"]:checked,
QPushButton[commonstyle = "textBtn"]:checked{

}

QAbstractButton[commonstyle = "textBtn"]:disabled,
QPushButton[commonstyle = "textBtn"]:disabled{
	border:1px solid #c4c4c4;
	color:#c4c4c4;
}

QTabWidget[commonstyle = "tabWidget"] {
	border: 1px solid #cccccc;
}

QTabWidget[commonstyle = "tabWidget"] QTabBar::tab{
    min-height: 24px;
    min-width:65px;
    padding:0 10px 0 10px;    
    color: #555555;
    background-color: white;
    border:1px solid #cccccc;
}

QTabWidget[commonstyle = "tabWidget"] QTabBar::tab:hover{
	background-color: rgb(189, 101, 12);
	color: white;
}

QTabWidget[commonstyle = "tabWidget"] QTabBar::tab:selected{
    border:1px solid #e68220;
    background-color: rgb(230, 131, 32);
    color: white;
}

QTabWidget[commonstyle = "tabWidget"] QTabBar::tab:!enabled{
    background:#c4c4c4;
}

QTabWidget[commonstyle = "tabWidget"] QTabBar::tab:!first{
    border:1px solid #cccccc;
	border-left:none;
}

QMenu[commonstyle = "smallMenu"] {
    background-color:white;
	margin:5px;
}

QMenu[commonstyle = "smallMenu"] QPushButton {
    padding-left:12px;
    padding-right:12px;
    border:none;
    background-color:white;
    min-height:24px;
    max-height:24px;
    font-family:"Microsoft YaHei";
	color:black;
}

QMenu[commonstyle = "smallMenu"] QPushButton:hover {
    background-color:#dbdbdb;
}

QMenu[commonstyle = "smallMenu"] QPushButton:pressed {
    background-color:#b7b7b7;
}

QMenu[commonstyle = "smallMenu"] QPushButton:disabled {
    background-color:white;
    color: #929292;
}

/*通用UI样式End*/


