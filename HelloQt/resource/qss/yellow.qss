QMenu[menu = "shortBtnOptions"] {
    background-color:white;
    margin:5px;
}

QMenu[menu = "shortBtnOptions"] QPushButton{
    background-image:none;
    background-position:left;
    padding-left:20px;
    padding-right:9px;
    background-repeat:no-repeat;
    background-origin:padding;
    text-align:left;
    border:none;
    border-left:6px solid white;
    background-color:white;
    min-height:24px;
    max-height:24px;
}

QMenu[menu = "shortBtnOptions"] QPushButton:checked{
    background-color:white;
    border:none;
    border-left:6px solid white;
    background-image: url(:/textanalysis/image-ht/textanalysis/shortcutbtn/ic_tick.png);
}

QMenu[menu = "shortBtnOptions"] QPushButton:hover{
    background-color:#dbdbdb;
    border:none;
    border-left:6px solid #dbdbdb;
    background-image:none;
}

QMenu[menu = "shortBtnOptions"] QPushButton:pressed{
    background-color:#b7b7b7;
	border:none;
    border-left:6px solid #b7b7b7;
    background-image:none;    
}

QWidget[backStyle = "white"],QDialog[backStyle = "white"]{
	background:white;
}

QMenu[menu = "customizedMenu"] {
    background-color:white;
    margin:5px;
}


QPushButton[btnStyle = "btn_close"]{
	background-image:url(:/homeWindow/image/homeWindow/ic_close2_normal.png);
	background-repeat:no-repeat;
	background-position:center;
	border:0px;
	margin: 5 5 5 5;
}

QWidget[borderStyle = "none_border"]
{
	border:0px;
}


QPushButton[btnStyle = "btn_close"]:hover,QPushButton[btnStyle = "btn_close"]:pressed{
	background-image:url(:/homeWindow/image/homeWindow/ic_close_hover.png);
	background-repeat:no-repeat;
	background-position:center;
	border:0px;
}

QWidget[backStyle = "trans"],QDialog[backStyle = "trans"]{
	background-color:transparent;
}

